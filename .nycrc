{
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": true,
  "all": true,
  "include": [
    "src/**/!(*.test.*).[tj]s?(x)"
  ],
  "exclude": [
    "src/_tests_/**/*.*"
  ],
  "reporter": [
    "html",
    "lcov",
    "text",
    "text-summary"
  ],
  "report-dir": "coverage",
  "temp-directory": "./coverage/.nyc_output",
  "branches": 80,
  "functions": 80,
  "lines": 80,
  "statements": 80
}
