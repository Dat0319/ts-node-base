{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Node: Nodemon",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/nodemon",
      "program": "${workspaceRoot}/src/index.ts",
      "restart": true,
      "protocol": "inspector",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/src/index.ts"
      ],
    },
  ]
}